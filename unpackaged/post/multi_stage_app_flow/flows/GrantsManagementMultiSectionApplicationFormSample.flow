            <targetReference>Application</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Display Default Section</defaultConnectorLabel>
        <rules>
            <name>DisplayAppOrgInfo</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>PendingStage</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>ApplicantOrganizationInformationStageLabel</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Application</targetReference>
            </connector>
            <label>Display Applicant Organization Info</label>
        </rules>
        <rules>
            <name>DisplayProposalSummary</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>PendingStage</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>ProposalSummaryStageLabel</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>ProposalSummarySection</targetReference>
            </connector>
            <label>Display Proposal Summary</label>
        </rules>
        <rules>
            <name>DisplayAttachments</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>PendingStage</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>AttachmentsStageLabel</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Attachments</targetReference>
            </connector>
            <label>Display Attachments</label>
        </rules>
        <rules>
            <name>DisplayReadOnlyDetails</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>UserRecordAccessMaxAccessLevel</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Read</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>FundingRequest.outfunds__Status__c</leftValueReference>
